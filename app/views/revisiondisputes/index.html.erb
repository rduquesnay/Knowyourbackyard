<h1>Listing revisiondiputes</h1>

<table>
  <thead>
    <tr>
      <th>Comment</th>
      <th>Revisionreview</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @revisiondisputes.each do |revisiondispute| %>
      <tr>
        <td><%= revisiondispute.comment %></td>
        <td><%= revisiondispute.revisionreview %></td>
        <td><%= link_to 'Show', revisiondispute %></td>
        <td><%= link_to 'Edit', edit_revisiondispute_path(revisiondispute) %></td>
        <td><%= link_to 'Destroy', revisiondispute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Revisiondispute', new_revisiondispute_path %>
