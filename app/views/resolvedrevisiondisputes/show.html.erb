<p id="notice"><%= notice %></p>
<h1 class="text-center">Resolved Revision Review Dispute</h1>
  <p>
  <strong>Trail Name: </strong>
  <%= @trail.name %>
  </p>
  <p>
    <strong>Review user:</strong>
    <%= @resolvedrevisiondispute.review_user_id %>
  </p>
<div class="well">
  <div class="row">
    <div class="col-md-3">
    </div>
    <div class="col-md-3">
      <h3>Revision Details</h3>
    </div>
    <div class="col-md-5">
      <h3>Review</h3>
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
      <p>
      <strong>Trail Name: </strong>
      </p>
    </div>
    <div class="col-md-3">
      <p>
      <%= @resolvedrevisiondispute.revisied_name %>
      </p>
    </div>
    <div class="col-md-5">
      <p>
      <%= @resolvedrevisiondispute.name_review %>
      </p>
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
      <p>
      <strong>Location: </strong>
      </p>
    </div>
    <div class="col-md-3">
      <p>
      <%= @resolvedrevisiondispute.revisied_location %>
      </p>
    </div>
    <div class="col-md-5">
      <p>
      <%= @resolvedrevisiondispute.location_review %>
      </p>
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
      <p>
      <strong>Season: </strong>
      </p>
    </div>
    <div class="col-md-3">
      <p>
      <%= @resolvedrevisiondispute.revisied_season %>
      </p>
    </div>
    <div class="col-md-5">
      <p>
      <%= @resolvedrevisiondispute.season_review %>
      </p>
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
      <p>
      <strong>Type: </strong>
      </p>
    </div>
    <div class="col-md-3">
      <p>
      <%= @resolvedrevisiondispute.revisied_trailtype %>
      </p>
    </div>
    <div class="col-md-5">
      <p>
      <%= @resolvedrevisiondispute.trailtype_review %>
      </p>
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
      <p>
      <strong>Length: </strong>
      </p>
    </div>
    <div class="col-md-3">
      <p>
      <%= @resolvedrevisiondispute.revisied_length %>
      </p>
    </div>
    <div class="col-md-5">
      <p>
      <%= @resolvedrevisiondispute.length_review %>
      </p>
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
      <p>
      <strong>GPS: </strong>
      </p>
    </div>
    <div class="col-md-3">
      <p>
      lat: <%= @resolvedrevisiondispute.revisied_latitude %>, long: <%= @resolvedrevisiondispute.revisied_longitude %>
      </p>
    </div>
    <div class="col-md-5">
      <p>
      <%= @resolvedrevisiondispute.gps_review %>
      </p>
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
      <p>
      <strong>Photos: </strong>
      </p>
    </div>
    <div class="col-md-3">
      <p>
      <div class="row">
        <% @images.files.each do |image| %>
          <div class="col-md-3">
            <%= link_to image.cdn_url do %>
              <%= image_tag image.cdn_url, width: 100 %>
            <%end%>
          </div>
        <%end%>
      </div>
      </p>
    </div>
    <div class="col-md-5">
      <p>
      <%= @resolvedrevisiondispute.photos_review %>
      </p>
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
      <p>
      <strong>Trail Directions/Description</strong>
      </p>
    </div>
    <div class="col-md-3">
      <p>
      <%= @resolvedrevisiondispute.revisied_traildirections %>
      </p>
    </div>
    <div class="col-md-5">
      <p>
      <%= @resolvedrevisiondispute.traildirections_review %>
      </p>
    </div>
  </div>
  <div class="well">
    <strong>Final Comments</strong><br>
    <p>
    <%= @resolvedrevisiondispute.finalcomments %>
    </p>
  </div>
</div>
<div class="well">
  <div class="row">
    <div class="col-md-10">
      <p>
        <%= @resolvedrevisiondispute.dispute_comment %>
      </p>
    </div>
  </div>
</div>
<div class="well">
  <p>
    <strong>Dispute action: </strong>
    <%= @resolvedrevisiondispute.dispute_action %>
  </p>
  <p>
    <strong>Comment: </strong><br>
    <%= @resolvedrevisiondispute.comment %>
  </p>
</div>
</p>

